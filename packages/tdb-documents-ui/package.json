    "cypress": "^10.10.0",
    "dotenv-webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "react-datetime-picker": "^4.2.0",
    "react-select-event": "^5.5.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
