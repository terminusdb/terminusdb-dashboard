{
  "name": "@terminusdb-live/tdb-react-components",
  "version": "6.0.1",
  "description": "components tools for terminusdb",
  "main": "src/index",
  "module": "es6/index",
  "jsnext:main": "es6/index",
  "sideEffects": [
    "./*/index.js"
  ],
  "files": [
    "*.md",
    "src",
    "es6",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "terminusdb"
  ],
  "scripts": {
    "build-es6": "rimraf es6 && cross-env NODE_ENV=es6 babel ./src -d es6 --extensions '.js' && babel ./src/css/less --out-dir es6/css/less --copy-files",
    "build": "npm run build-es6 && rimraf dist && webpack",
    "start01": "webpack serve --mode development  --open --progress --port 3000 --host localhost --config demo/webpack.config.js",
    "start": "webpack serve --mode development  --open --progress --port 3030 --host localhost --entry ./src --config ./webpack_dev.config.js --history-api-fallback",
    "test": "echo 'TODO: There should be tests created'",
    "lint": "eslint src",
    "autofix": "eslint src --fix"
  },
  "pre-commit": [],
  "pre-push": [
    "test",
    "lint"
  ],
  "repository": {},
  "author": "terminusdb group",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@terminusdb/terminusdb-client": "^10.0.25",
    "babel-jest": "^29.3.1",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.5",
    "codemirror": "^5.59.1",
    "core-js": "^3.8.2",
    "d3": "^5.16.0",
    "date-fns": "^2.16.1",
    "install": "^0.13.0",
    "moment": "^2.27.0",
    "prop-types": "^15.6.0",
    "re-resizable": "^6.9.0",
    "react-bootstrap": "^2.7.0",
    "react-codemirror2": "^7.2.1",
    "react-dates": "^21.8.0",
    "react-hook-form": "^6.14.1",
    "react-icons": "^3.11.0",
    "react-json-view": "^1.21.3",
    "react-motion": "^0.5.2",
    "react-numeric-input": "^2.2.3",
    "react-outside-click-handler": "^1.3.0",
    "react-responsive-tabs": "^4.1.1",
    "react-select": "^3.1.0",
    "react-sizeme": "^2.6.12",
    "react-split-pane": "^0.1.92",
    "react-table": "^7.6.3",
    "react-textarea-autosize": "^8.3.0",
    "react-tooltip": "^4.2.10",
    "react-transition-group": "^4.4.1",
    "react-use-dimensions": "^1.2.1",
    "recharts": "^1.8.5",
    "ts-jest": "^29.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.0"
  },
  "license": "APACHE"
}
