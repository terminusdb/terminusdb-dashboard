  "description": "react hook library for manage terminusdb live workers",
  "main": "src/index",
  "module": "es6/index",
  "jsnext:main": "es6/index",
  "sideEffects": [
    "./*/index.js"
  ],
  "files": [
    "*.md",
    "src",
    "es6"
  ],
  "keywords": [
    "react",
    "reactjs",
    "chart",
    "react-component",
    "terminusdb"
  ],
  "scripts": {
    "build-es6": "rimraf es6 && babel ./src -d es6 --extensions '.js'",
    "build": "npm run build-es6 && webpack --mode production",
